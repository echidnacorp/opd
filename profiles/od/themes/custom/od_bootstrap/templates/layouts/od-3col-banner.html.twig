<div class="row">
  {% if content.banner_left %}
  <div class="col-md-6">
    {{ content.banner_left }}
  </div>
  {% endif %}
  {% if content.banner_right %}
  <div class="col-md-6">
    {{ content.banner_right }}
  </div>
  {% endif %}
</div>
<div class="row">
  {% if content.top_left %}
  <div class="col-md-4">
    {{ content.top_left }}
  </div>
  {% endif %}
  {% if content.top_middle %}
  <div class="col-md-4">
    {{ content.top_middle }}
  </div>
  {% endif %}
  {% if content.top_right %}
  <div class="col-md-4">
    {{ content.top_right }}
  </div>
  {% endif %}
</div>
<div class="row">
  {% if content.middle %}
  <div class="col-md-12">
    {{ content.middle }}
  </div>
  {% endif %}
</div>
<div class="row">
  {% if content.bottom_left %}
  <div class="col-md-4">
    {{ content.bottom_left }}
  </div>
  {% endif %}
  {% if content.bottom_middle %}
  <div class="col-md-4">
    {{ content.bottom_middle }}
  </div>
  {% endif %}
  {% if content.bottom_right %}
  <div class="col-md-4">
    {{ content.bottom_right }}
  </div>
  {% endif %}
</div>
<div class="row">
  {% if content.bottom %}
  <div class="col-md-12">
    {{ content.bottom }}
  </div>
  {% endif %}
</div>
